; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "SaveLoad",
"language": "GDScript",
"path": "res://ressources/SaveLoad.gd"
}, {
"base": "Panel",
"class": "TextPanel",
"language": "GDScript",
"path": "res://scripts/components/TextPanel.gd"
} ]
_global_script_class_icons={
"SaveLoad": "",
"TextPanel": ""
}

[application]

config/name="Tempo/TÃ¢ches"
run/main_scene="res://scenes/Main.tscn"
boot_splash/image="res://logo_tempo_taches_android.png"
config/icon="res://logo_tempo_taches.png"

[audio]

driver=""
enable_audio_input=true

[display]

window/size/width=320
window/size/height=640
window/size/resizable=false
window/size/borderless=true
window/size/fullscreen=true
window/handheld/orientation="portrait"
window/stretch/mode="2d"

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": false,
"flags/anisotropic": true,
"flags/filter": false,
"flags/mipmaps": true,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": true,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/normal_map_invert_y": false,
"process/premult_alpha": true,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}
wav={
"compress/mode": 0,
"edit/loop_begin": 0,
"edit/loop_end": -1,
"edit/loop_mode": 0,
"edit/normalize": false,
"edit/trim": true,
"force/8_bit": false,
"force/max_rate": false,
"force/max_rate_hz": 44100,
"force/mono": false
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_s3tc=false
vram_compression/import_etc=true
vram_compression/import_etc2=false
vram_compression/import_pvrtc=true
gles2/compatibility/disable_half_float.iOS=false
environment/default_environment="res://default_env.tres"
